package com.xue.study.DataArray;


public class ArrayTest {
	public static void main(String[] args) {
		 int[] array = new int[10];
		 for (int i = 0; i < 7; i++) 
			array[i] = i;
		 for (int i : array) 
			 System.out.println(i);
		 int[] scores = {10, 99, 66};
		 for (int i = 0; i < scores.length; i++)
			 System.out.println(scores[i]);
		 System.out.println("---------");
		 for (int i : scores)
			 System.out.println(i);
	}
}

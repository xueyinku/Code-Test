package com.xue.test;

import java.util.Iterator;

public class MyThread extends Thread{
	private String name;
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return name;
	}
	
	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("run:" + i);
		}
	}
}
